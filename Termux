let archivo = fileURLToPath(import.meta.url)
watchFile(archivo, () => {
unwatchFile(archivo)
console.log(chalk.redBright("Se actualiza 'config.js'"))
importar(`${archivo}?actualización=${Fecha.now()}`)
})
